{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Content": {
      "type": "dynamiczone",
      "components": [
        "content.content",
        "image.medias"
      ]
    },
    "title": {
      "type": "string"
    },
    "articleicon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "articlecover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "article_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::article-category.article-category",
      "inversedBy": "articles"
    },
    "subtitle": {
      "type": "string"
    },
    "articleBackgroundCoverColor": {
      "type": "string",
      "maxLength": 6,
      "minLength": 3,
      "regex": "[A-Fa-f0-9]"
    },
    "articleTextCoverColor": {
      "type": "string"
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    }
  }
}
